-- main

state_stack = {}

function _init()
	palt(0,false)
 palt(14,true)
 
 state_stack = {}
 add(state_stack, state_mission())
end

function _update()
	for i=0,3 do
		if btnp(i) then
			state_stack[#state_stack]:handle_input(i)	
		end
	end
end

function _draw()
 cls()
	for s in all(state_stack) do
		s:draw()
	end
end
-->8
-- entities

function player()
	local player = {
		x=2,
		y=7,
		spr=1,
		wep=19,
		flip=false,
	}
	
	player.draw=function(self,map_x)
		local p_x=self.x-map_x*16
		local p_y=self.y+1
		
		spr(self.spr,p_x*8,p_y*8,1,1,self.flip) 
		spr(self.wep,p_x*8,p_y*8,1,1,self.flip)
	end
	
	player.move=function (self, d)
		-- flip if moved laterally
		if (d[1]<0) self.flip=true
		if (d[1]>0) self.flip=false
		
		local destx=self.x+d[1]
		local desty=self.y+d[2]
			
		-- if tile is blocked
		if fget(mget(destx,desty),0) then
			return
		end
		
		-- update player pos
		self.x,self.y=destx,desty
	end
	
	return player
end




-->8
-- states

-- mission
function state_mission()
	local state = {}
	
	state.data={
		player=player(),
		map_x=0,
	}
	
	state.handle_input=function(self,b)
		local d=self.data
		
		d.player:move(dirs[b+1])
		d.map_x=flr(d.player.x/16)
	end
	
	state.draw=function(self)
		local d=self.data
		map(d.map_x*16,0,0,8)
	 d.player:draw(d.map_x)
		rectfill(0,0,128,7,1)
		print("hp",2,2,7)
		spr(33, 10, 0)
		spr(33, 18, 0)
		spr(34, 26, 0)
	end
		
	return state
end
